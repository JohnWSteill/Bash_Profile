# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
        . ~/.bashrc
fi

# User specific environment and startup programs

export PERL_LOCAL_LIB_ROOT=/isitools/perl5.26.1/lib/5.26.1
export PERL_MB_OPT=/isitools/perl5.26.1/lib/5.26.1
export PERL_MM_OPT=INSTALL_BASE=/isitools/perl5.26.1/lib/5.26.1
#export PERL5LIB='/isitools/perl5.26.1/lib/5.26.1/lib/perl5:/isitools/perl5.26.1/lib/5.26.1/x86_64-linux:/isitools/perl5.26.1/lib/5.26.1:/isitools/perl5.26.1/lib/5.26.1/x86_64-linux-thread-multi-ld'
export PERL5LIB=/isitools/perl5.26.1/lib/5.26.1
export PATH=/isitools/perl5.26.1:/isitools/perl5.26.1/bin:/isitools/perl5.26.1/lib/5.26.1:$PATH:

echo "BE SURE TO export HOME=/isitools/perl5.26.1 IF MAKING CPAN MODS"
