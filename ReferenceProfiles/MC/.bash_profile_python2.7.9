# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
        . ~/.bashrc
fi

# User specific environment and startup programs

export PATH=/isitools/Python2.7.9/bin:/isitools/Python2.7.9/lib/python2.7/site-packages:$PATH
export WORKON_HOME=$HOME/.virtualenvs
export PYTHONPATH=/isitools/Python2.7.9/bin:$PYTHONPATH
export VIRTUALENVWRAPPER_PYTHON=/isitools/Python2.7.9/bin/python
export VIRTUALENVWRAPPER_VIRTUALENV=/isitools/Python2.7.9/bin/virtualenv
source /isitools/virtualenvwrapper-3.6/virtualenvwrapper.sh

